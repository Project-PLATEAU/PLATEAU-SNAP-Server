{
    "metadata": [
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "PLATEAU SNAP Server",
            "_appFooter": "PLATEAU",
            "_enableSearch": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "manual",
                "files": [
                    "toc.yml",
                    "*.md"
                ],
                "dest": "manual"
            }
        ],
        "resource": [
            {
                "files": [
                    "resources/**/*"
                ]
            }
        ],
        "dest": "../.docs/_site"
    }
}
